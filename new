public class SwitchDemo
{
  public void a(int month)
  {
    int day=0;
    switch(month)
    {
      case  2:
      day=28;
      break;
      case  4:
      case  6:
      case  9:
      case  11:
      day=30;
      break;
      default:
      day=31;
    }
    System.out.printIn(month+"th month have"+day+"days");
  }
  public static main(args[])
  {SwitchDemo obj=new SwitchDemo();
  obj.a(3);
  }
}
