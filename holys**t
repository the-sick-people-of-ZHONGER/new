public class Ifdemo
{
  public static void main(String args[])
  {
    int n=97;
    String s;
    if(i>=90)
      s="prefect";
    else if(i>=80)
      s="great";
      else if(i>=70)
        s="good";
        else if(i>=60)
          s="bad"
